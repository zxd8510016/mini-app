<template name="star-template">
  <view class="star-box">
    <view class="star">
      <block wx:for="{{stars}}" wx:key="{{index}}">
        <image wx:if="{{item}}" src="/images/icon/star.png"></image>
        <image wx:else src="/images/icon/none-star.png"></image>
      </block>
    </view>
    <text class="star-score">{{average}}</text>
  </view>
</template>